{
    "version": 2,
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "headers": [
      {
        "source": "/models/(.*)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "model/gltf-binary"
          }
        ]
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }